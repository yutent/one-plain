{
  "name": "米白色主题",
  "type": "light",
  "tokenColors": [
    {
      "settings": {
        "background": "#fafafa",
        "foreground": "#546e7a"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#B6D5E0"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#546e7a"
      }
    },
    {
      "name": "PHP Constants",
      "scope": ["constant.other.php"],
      "settings": {
        "foreground": "#FFB62C"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#546e7a"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#546e7a"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFB62C"
      }
    },
    {
      "name": "Unimplemented",
      "scope": "invalid.unimplemented",
      "settings": {
        "foreground": "#546e7a"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["storage.type", "keyword.control"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
